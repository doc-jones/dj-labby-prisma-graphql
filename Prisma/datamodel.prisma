type SlackMessage {
  id: ID! @id
  createdAt: DateTime! @createdAt
  text: String!
  owner: People! @relation(link: INLINE)
  location: Location!
}

type People {
  id: ID! @id
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
  handle: String! @unique
  github: String! @unique
  first_name: String
  last_name: String
  timezone: String
  program: String
}

type Role {
  id: ID! @id
  name: String
  type: String
}

type Group {
    id: ID! @id
    name: String
}

type Products {
    id: ID! @id
    name: String
}

type Projects {
    id: ID! @id
    productid: String
    name: String
    start: DateTime! @start
    end: DateTime! @end
}